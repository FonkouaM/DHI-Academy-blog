{% extends 'base.html.twig' %}

{% block title %}Hello EtudiantsController!{% endblock %}

{% block body %}
<center>
<h1>Liste des etudiants
</h1>
<table>
<tr>
<th>Matricule
</th>
<th>Nom
</th>
<th>Age
</th>
<th>Sexe
</th>
</tr>
{% for etudiant in etudiants %}
<tr>
<td>{{etudiant.matricule}}
</td>
<td>{{etudiant.nom}}
</td>
<td>{{etudiant.age}}
</td>
<td>{{etudiant.sexe}}
</td>
</tr>
{% endfor %}
</table>
</center>
{% endblock %}
